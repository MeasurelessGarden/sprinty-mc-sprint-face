{
  "name": "sprinty-mc-sprintface",
  "version": "0.3.0",
  "description": "Measureless Measures Your Words",
  "main": "src/main.js",
  "author": "Measureless",
  "scripts": {
    "bot": "SPRINTY_VERSION=$npm_package_version NODE_ENV=test babel-node src/main.js",
    "test": "SPRINTY_VERSION=test-version NODE_ENV=test mocha --recursive ./test",
    "format": "find src test -name '*.js' | xargs ./node_modules/.bin/prettier --config .prettierrc.json --write",
    "version": "echo $npm_package_version"
  },
  "dependencies": {
    "babel-cli": "^6.0.0",
    "discord.js": "latest",
    "lodash": "latest"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^4.0.1",
    "sinon": "^4.1.2",
    "prettier-miscellaneous": "^1.5.3",
    "unroll": "latest"
  }
}
